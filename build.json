{
    "ModuleName": "FleetDM-PowerShell",
    "Author": "Jorge Suarez",
    "CompanyName": "Jorgeasaurus",
    "Copyright": "(c) 2025 Jorgeasaurus. All rights reserved.",
    "Description": "PowerShell module for FleetDM API integration. Provides cmdlets for managing hosts, queries, policies, and software through the FleetDM REST API.",
    "PowerShellVersion": "5.1",
    "CompatiblePSEditions": ["Desktop", "Core"],
    "Tags": ["FleetDM", "MDM", "Security", "API", "DeviceManagement", "Osquery"],
    "ProjectUri": "https://github.com/Jorgeasaurus/FleetDM-PowerShell",
    "LicenseUri": "https://github.com/Jorgeasaurus/FleetDM-PowerShell/blob/main/LICENSE",
    "ReleaseNotes": "See https://github.com/Jorgeasaurus/FleetDM-PowerShell/releases",
    "BuildHelpers": {
        "Version": "2.0.0"
    },
    "Pester": {
        "Version": "5.0.0",
        "Configuration": {
            "Output": {
                "Verbosity": "Detailed"
            },
            "TestResult": {
                "Enabled": true,
                "OutputFormat": "NUnitXml",
                "OutputPath": "Output/TestResults.xml"
            },
            "CodeCoverage": {
                "Enabled": true,
                "OutputFormat": "JaCoCo",
                "OutputPath": "Output/CodeCoverage.xml",
                "CoveragePercentTarget": 70
            }
        }
    },
    "PSScriptAnalyzer": {
        "Version": "1.19.0",
        "SettingsFile": "build/PSScriptAnalyzerSettings.psd1"
    },
    "ModuleBuildTasks": {
        "Encoding": "UTF8",
        "StripComments": false,
        "UpdateModuleVersion": true,
        "UpdateFunctionExports": true
    }
}